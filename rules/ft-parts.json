{
    "id": "ft-parts",
    "name": "FT Parts Summary",
    "description": "Extrae c√≥digos y cantidades en FT y los agrupa",
    "extract": {
        "type": "regex",
        "pattern": "\\b([A-Z]{2,4}\\d{5,})\\b[\\s\\S]{0,80}?(\\d+(?:\\.\\d+)?)\\s*FT\\b",
        "fields": [
            "part_number",
            "qty"
        ]
    },
    "groupBy": "part_number",
    "sumField": "qty",
    "uom": "FT",
    "output": {
        "format": "text"
    }
}